  "license": "MIT",
  "main": "src",
  "dependencies": {
    "antlr4": "^4.7.1",
    "is-alphanumerical": "^1.0.2",
    "lodash": "^4.17.10",
    "prettier": "^1.12.1"
  },
  "scripts": {
    "test:debug": "node --lazy --inspect-brk test",
